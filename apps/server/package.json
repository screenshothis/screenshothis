{
	"name": "@screenshothis/server",
	"type": "module",
	"imports": {
		"#/*": [
			"./src/*"
		]
	},
	"exports": {
		"./routers": "./src/routers/index.ts"
	},
	"main": "src/index.ts",
	"scripts": {
		"build": "tsc && tsc-alias",
		"compile": "bun build --compile --minify --sourcemap --bytecode ./src/index.ts --outfile server",
		"db:push": "drizzle-kit push",
		"db:reset": "bun run scripts/reset-db.ts",
		"db:studio": "drizzle-kit studio",
		"dev": "bun run --hot src/index.ts",
		"start": "bun run dist/src/index.js",
		"typecheck": "tsc --noEmit"
	},
	"dependencies": {
		"@ai-sdk/google": "1.2.18",
		"@ghostery/adblocker-puppeteer": "2.6.1",
		"@hono-storage/core": "0.0.14",
		"@hono-storage/s3": "0.0.13",
		"@hono/swagger-ui": "0.5.1",
		"@hono/zod-openapi": "0.19.8",
		"@orpc/client": "1.3.0",
		"@orpc/server": "1.3.0",
		"@polar-sh/better-auth": "1.0.1",
		"@polar-sh/sdk": "0.32.16",
		"@screenshothis/id": "workspace:*",
		"@screenshothis/schemas": "workspace:*",
		"@t3-oss/env-core": "0.13.6",
		"ai": "4.3.16",
		"better-auth": "1.2.8",
		"better-auth-harmony": "1.2.5",
		"bullmq": "5.53.1",
		"cross-fetch": "4.1.0",
		"dotenv": "16.5.0",
		"drizzle-orm": "0.44.1",
		"hono": "4.7.11",
		"hono-pino": "0.8.0",
		"p-limit": "6.2.0",
		"pg": "8.16.0",
		"pino": "9.7.0",
		"puppeteer": "24.9.0",
		"puppeteer-extra": "3.3.6",
		"puppeteer-extra-plugin-stealth": "2.11.2",
		"svix": "1.66.0",
		"ts-case-convert": "2.1.0",
		"unique-username-generator": "1.4.0",
		"wildcard-match": "5.1.4",
		"zod": "3.25.46"
	},
	"devDependencies": {
		"@epic-web/config": "1.21.0",
		"@types/bun": "1.2.15",
		"@types/pg": "8.15.3",
		"drizzle-kit": "0.31.1",
		"tsc-alias": "1.8.16",
		"typescript": "5.8.3"
	},
	"packageManager": "bun@1.2.15"
}
