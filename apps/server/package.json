{
	"name": "server",
	"type": "module",
	"imports": {
		"#/*": "./src/*"
	},
	"main": "src/index.ts",
	"scripts": {
		"build": "tsc && tsc-alias",
		"check-types": "tsc --noEmit",
		"compile": "bun build --compile --minify --sourcemap --bytecode ./src/index.ts --outfile server",
		"db:push": "drizzle-kit push",
		"db:studio": "drizzle-kit studio",
		"dev": "bun run --hot src/index.ts",
		"start": "bun run dist/src/index.js"
	},
	"dependencies": {
		"@ai-sdk/google": "1.2.14",
		"@clerk/backend": "1.31.2",
		"@ghostery/adblocker-playwright": "2.5.2",
		"@hono-storage/core": "0.0.14",
		"@hono-storage/s3": "0.0.13",
		"@hono/clerk-auth": "2.0.0",
		"@hono/swagger-ui": "0.5.1",
		"@hono/trpc-server": "0.3.4",
		"@hono/zod-openapi": "0.19.6",
		"@trpc/client": "11.1.2",
		"@trpc/server": "11.1.2",
		"ai": "4.3.13",
		"base-x": "5.0.1",
		"cross-fetch": "4.1.0",
		"dotenv": "16.5.0",
		"drizzle-orm": "0.43.1",
		"hono": "4.7.8",
		"hono-openapi": "0.4.8",
		"pg": "8.15.6",
		"playwright-extra": "4.3.6",
		"puppeteer-extra-plugin-stealth": "2.11.2",
		"svix": "1.64.1",
		"ts-case-convert": "2.1.0",
		"zod": "4.0.0-beta.20250505T195954"
	},
	"devDependencies": {
		"@epic-web/config": "1.20.1",
		"@types/bun": "1.2.12",
		"@types/pg": "8.11.14",
		"drizzle-kit": "0.31.1",
		"tsc-alias": "1.8.16",
		"typescript": "5.8.3"
	},
	"packageManager": "bun@1.2.12"
}
