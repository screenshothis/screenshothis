{
	"name": "@screenshothis/server",
	"type": "module",
	"imports": {
		"#/*": [
			"./src/*"
		]
	},
	"exports": {
		"./routers": "./src/routers/index.ts"
	},
	"main": "src/index.ts",
	"scripts": {
		"build": "tsc && tsc-alias",
		"compile": "bun build --compile --minify --sourcemap --bytecode ./src/index.ts --outfile server",
		"db:push": "drizzle-kit push",
		"db:reset": "bun run scripts/reset-db.ts",
		"db:studio": "drizzle-kit studio",
		"dev": "bun run --hot src/index.ts",
		"start": "bun run dist/src/index.js",
		"typecheck": "tsc --noEmit"
	},
	"dependencies": {
		"@ai-sdk/google": "1.2.18",
		"@ghostery/adblocker-playwright": "2.5.2",
		"@ghostery/adblocker-puppeteer": "2.5.2",
		"@hono-storage/core": "0.0.14",
		"@hono-storage/s3": "0.0.13",
		"@hono/swagger-ui": "0.5.1",
		"@hono/zod-openapi": "0.19.6",
		"@orpc/client": "1.2.0",
		"@orpc/server": "1.2.0",
		"@polar-sh/hono": "0.3.0",
		"@screenshothis/schemas": "workspace:*",
		"@unkey/api": "0.36.0",
		"@unkey/hono": "1.4.16",
		"ai": "4.3.15",
		"base-x": "5.0.1",
		"better-auth": "1.2.7",
		"cross-fetch": "4.1.0",
		"dotenv": "16.5.0",
		"drizzle-orm": "0.43.1",
		"hono": "4.7.9",
		"p-limit": "6.2.0",
		"pg": "8.15.6",
		"playwright-extra": "4.3.6",
		"puppeteer": "24.8.2",
		"puppeteer-extra": "3.3.6",
		"puppeteer-extra-plugin-stealth": "2.11.2",
		"svix": "1.65.0",
		"ts-case-convert": "2.1.0",
		"unique-username-generator": "1.4.0",
		"wildcard-match": "5.1.4",
		"zod": "3.24.4"
	},
	"devDependencies": {
		"@epic-web/config": "1.20.1",
		"@types/bun": "1.2.13",
		"@types/pg": "8.15.1",
		"drizzle-kit": "0.31.1",
		"tsc-alias": "1.8.16",
		"tsdown": "0.11.5",
		"typescript": "5.8.3"
	},
	"packageManager": "bun@1.2.13"
}
