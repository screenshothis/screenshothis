# MinIO Configuration for Local Development
MINIO_ROOT_USER="minio-root-user"
MINIO_ROOT_PASSWORD="minio-root-password"
# These credentials match the Docker Compose MinIO setup
AWS_ACCESS_KEY_ID=screenshothis-access-key
AWS_SECRET_ACCESS_KEY=screenshothis-secret-key
AWS_REGION=us-east-1
AWS_BUCKET=screenshothis-bucket
# MinIO service endpoint for API calls
AWS_URL=http://localhost:9000
AWS_ENDPOINT=http://localhost:9000
# Alternative endpoint configuration (some SDKs prefer this)
AWS_USE_PATH_STYLE_ENDPOINT=true
